        "#numpyの読み込み\n",
        "import numpy as np\n",
        "\n",
        "a = np.array([[0, 1], [2, 3], [4, 5]])\n",
        "b = np.array([[0, 1, 2, 3], [4, 5, 6, 7]])\n",
        "\n",
        "#行列の掛け算\n",
        "culiculation_result_array = np.dot(a, b)\n",
        "\n",
        "#計算結果から最大を求める\n",
        "culiculation_result_array.max()"
      ]
